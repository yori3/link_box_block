{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/link-box-block",
  "version": "0.1.0",
  "title": "Link Box Block",
  "icon": "universal-access-alt",
  "category": "design",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "href": {
      "type": "string",
      "default": ""
    },
    "target": {
      "type": "boolean",
      "default": false
    }
  },
  "description": "ボックス全体にリンクを設定できるブロック。",
  "example": {},
  "supports": {
    "html": false,
    "anchor": true
  },
  "textdomain": "link-box-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}